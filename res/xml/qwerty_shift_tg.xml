    <Row android:keyWidth="9.091%p">
        <Key android:codes="0x0C13" 
                android:keyEdgeFlags="left"/>
        <Key android:codes="0x0C0F" />
        <Key android:codes="0x0C05" />
        <Key android:codes="0x0C07" />
        <Key android:codes="0x0C09" />
        <Key android:codes="0x0C2B" />
        <Key android:codes="0x0C31" />
        <Key android:codes="0x0C16" />
