<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row android:keyWidth="9.091%p">
        <Key android:codes="0x0C14" android:keyEdgeFlags="left"/>
        <Key android:codes="0x0C10" />
        <Key android:codes="0x0C06" />
        <Key android:codes="0x0C08" />
        <Key android:codes="0x0C0A" />
        <Key android:codes="0x0C2D" />
      	<Key android:codes="0x0C19" />
        <Key android:codes="0x0C18" />
        <Key android:codes="0x0C27" />
        <Key android:codes="0x0C1D" />
        <Key android:codes="0x0C22" android:keyEdgeFlags="right"/>
