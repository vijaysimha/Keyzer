<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row android:keyWidth="9.091%p">
        <Key android:codes="0x0994" android:keyEdgeFlags="left"/>
        <Key android:codes="0x0990" />
        <Key android:codes="0x0986" />
        <Key android:codes="0x0988" />
        <Key android:codes="0x098A" />
        <Key android:codes="0x09AD" />
      	<Key android:codes="0x0999" />
        <Key android:codes="0x0998" />
        <Key android:codes="0x09A7" />
        <Key android:codes="0x099D" />
        <Key android:codes="0x09A2" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:keyWidth="9.091%p">
        <Key android:codes="0x0993" 
                android:keyEdgeFlags="left"/>
        <Key android:codes="0x098F" />
        <Key android:codes="0x0985" />
        <Key android:codes="0x0987" />
        <Key android:codes="0x0989" />
        <Key android:codes="0x09AB" />
        <Key android:codes="0x09B0" />
        <Key android:codes="0x0996" />
        <Key android:codes="0x09A5" />
        <Key android:codes="0x099B" />
        <Key android:codes="0x09A0"  android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
                android:keyWidth="15%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="0x09FA" />
        <Key android:codes="0x09A3" />
        <Key android:codes="0x0983" />
        <Key android:codes="0x098B" />
        <Key android:codes="0x09B6" />
        <Key android:codes="0x09B7" />
        <Key android:codes="0x09DF" />
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="15%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" 
                android:keyWidth="20%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="123" android:keyWidth="15%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="46,44" android:keyLabel=". ,"
                android:keyWidth="15%p"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="20%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
 